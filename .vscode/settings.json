{
  "emmet.includeLanguages": { "erb": "html" },
  "[ruby][javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": { "source.fixAll": true },
  "eslint.validate": ["javascript", "html", "erb", "vue"],
  "eslint.options": { "extensinons": [".js", ".html", ".erb", ".vue"] }
}
