<%= simple_form_for [:admin, @schedule] do |f| %>
  <%= f.error_notification %>
  <%= f.association :team, collection: Service::Team.all.pluck(:name, :id), prompt: :translate %>
  <%= f.input :service_time %>
  <%= button_tag class: 'btn btn-primary', data: { disable_with: t('button.submitting') } do %>
    <%= fa_icon('floppy-o', text: t('button.submit')) %>
  <% end %>
<% end %>
